procedure spokes(x, y, r, i, angle,s,c)
   local incr, j

   incr := 2 * &pi / i 
  Fg(c)
   every j := 1 to i do {
DrawSphere(x + r * cos(angle),0,y + r * sin(angle), s)

      angle +:= incr
      }

   return

#radii..use x,y,r
   
end

