WWrite(win1,"Cube is 11 values: X = 001, Y = 001, Z = 001, size = 1 and color value = 1.")
WWrite(win1,"Sphere is 11 values: X = 001, Y = 001, Z = 001, size = 1 and color value = 1.")
WWrite(win1,"Cylinder is 13 values: X = 001, Y = 001, Z = 001, size = 1, end 1, end 2 and color value = 1.")
WWrite(win1,"Sphere Random Array is is 11 values: X = 001, Y = 001, Z = 001, size = 1 and color value = 1.")
