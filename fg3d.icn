procedure inputprompt()
 
        s1 := WRead()

        ss1 :=( "cube")
        ss2 :=( "plane")
        ss3 :=( "cyl")
        ss4 :=( "sphere")
        ss5 :=( "psplane")
        ss6 :=( "notused")
        ss7 :=( "notused")
        ss8 :=( "notused")
        ss9 :=( "notused")
        ss10 :=( "notused")
        ss11 :=( "notused")
        ss12 :=( "notused")
        ss13 :=( "notused")
        ss14 :=( "notused")
        ss15 :=( "notused")
        ss16 :=( "notused")
        ss17 :=( "notused")
        ss18 :=( "cam")
        ss19 :=( "vox1")
        ss20 :=( "vox2")
        ss21 :=( "int")
        ss22 :=( "notused")
        ss23 :=( "notused")
      

        if s1== ss1 then inputCube01() 
        else
        if s1== ss2 then inputPlane01() 
        else
        if s1== ss3 then inputCyl01() 
        else
        if s1== ss4 then inputSphere01() 
        else
        if s1== ss5 then psplane01()
        else
        if s1== ss6 then notused 
        else
        if s1== ss7 then notused 
        else
        if s1== ss8 then notused 
        else
        if s1== ss9 then notused
        else
        if s1== ss10 then notused
        else
        if s1== ss11 then notused
        else
        if s1== ss12 then notused
        else
        if s1== ss13 then notused
        else
        if s1== ss14 then notused
        else
        if s1== ss15 then notused
        else
        if s1== ss16 then notused
        else
        if s1== ss17 then notused
        else
        if s1== ss18 then seteyein()
        else
        if s1== ss19 then voxin01()
        else
        if s1== ss20 then voxin02()
        else
        if s1== ss21 then int()
        else
        if s1== ss22 then notused
        else
        if s1== ss23 then notused 

    return

end

