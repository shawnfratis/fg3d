#begin interpereter

global WHITE,MFLAG,PRTBL	

procedure int(arg)
   
   local line, lno, pline
   WClose()

	WHITE := ' \t\n\f'

	PRTBL := table()

	MFLAG := 0

       writes("Input> ")
	while line := read() do {
       
		line ? {

			tab(many(WHITE))

			if lno := tab(many(&digits)) & tab(many(WHITE)) then {

				pline := tab(0)

				PRTBL[numeric(lno)] := pline

				MFLAG +:= 1
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("read" | "r") then {
				readprog()

				MFLAG := 0
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("write" | "w") then {
				writeprog()

				MFLAG := 0
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("delete" | "d") then {
				delprog()

				MFLAG +:= 1
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("sequence" | "s") then {
				seqprog()
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("list" | "l") then {
				listprog()
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("execute" | "e") then {
				execprog()
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("help" | "h" | "?") then {
				helpprog()
			}

			else if (tab(upto(WHITE)) | tab(0)) ==
				("quit" | "q") then {
				quitprog()
			}

			else {
			       	write("Syntax Error: ",line)
                               # WWrite("Syntax Error: ",line)
