		"list [ begno [ endno ] ]",
		"delete [ begno [ endno ] ]",
		"sequence [ begno [ increment ] ]",
		"execute [ args ]",
		"quit",
		"help"
		]
	}

	every write(!helpmsg)

end

procedure commandlist()

	static comlst

	initial {
		comlist := [
		"<<< command list goes here?>>>",
	
		]
	}

	every write(!comlist)

end

procedure quitprog()
   local response

	if MFLAG > 0 then {
		writes("Write before quitting? ")
		response := read()
		if any('yY',response) then
			writeprog()
	}

	stop("Goodbye.")

end

