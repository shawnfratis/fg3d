procedure scene01(b)

   (xc := 0,yc := 0)

   i := 15

   size := b #was 10
   radius := size / .8
   xc := yc := size / .8

   angle := 0.0
   incr := 2 * &pi / i

   every j := 1 to i do {
      spokes(xc + radius * cos(angle), yc + radius * sin(angle),
      radius, i, angle)
      angle +:= incr
      }
 
end

procedure spokes(x, y, r, i, angle)
   local incr, j

   incr := 2 * &pi / i 
  Fg("red")
   every j := 1 to i do {
DrawCube(x + r * cos(angle),y + r * sin(angle), 0, .5)

      angle +:= incr
      }

   return

#radii..use x,y,r
   
end

