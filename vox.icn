	static helpmsg

	initial {
		helpmsg := [
		"<<< Icon-based Expression Syntax >>>",
		"",
		"lineno expression",
		"",
		"<<< Command Summary >>>",
		" (1st character works)",
		"",
		"read [ file ]",
